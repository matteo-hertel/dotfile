dotfiles(){
  NAME=dotfiles
  echo $NAME
  tmux has-session -t $NAME
  if [ $? != 0 ]
  then
    tmux new-session -d -s $NAME -n nvim
    tmux send-keys -t $NAME:0 "cd ~/mhdev/dotfiles;nvim" C-m 
    tmux splitw -t $NAME:0 -h -p 35
    tmux selectp -t $NAME:0
  fi
  if [ -z $TMUX ]
  then
    tmux attach -t $NAME
  fi
    tmux switch -t $NAME
}
alias dotfiles=dotfiles
